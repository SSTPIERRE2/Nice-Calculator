{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "rules": {
    "no-undef": 0,
    "no-static-element-interactions": 0,
    "arrow-body-style": 0,
    "no-nested-ternary": 0
  }
}